{
  "name": "mcp-servers",
  "version": "1.0.0",
  "description": "Collection of Model Context Protocol (MCP) servers",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:calcs-api",
    "build:calcs-api": "cd calcs-api && npm run build",
    "dev:calcs-api": "cd calcs-api && npm run dev",
